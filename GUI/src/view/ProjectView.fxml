            <TabPane nodeOrientation="RIGHT_TO_LEFT" prefHeight="369.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                  <Tab text="Settings">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <VBox alignment="TOP_CENTER" prefHeight="292.0" prefWidth="185.0">
                                 <children>
                                    <Button mnemonicParsing="false" text="Export projects to XML">
                                       <VBox.margin>
                                          <Insets top="10.0" />
                                       </VBox.margin>
                                    </Button>
                                 </children>
                              </VBox>
                           </children></AnchorPane>
                    </content>
                  </Tab>
                  <Tab text="Past">
                     <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="268.0" prefWidth="600.0">
                           <children>
                              <HBox prefHeight="46.0" prefWidth="600.0">
                                 <children>
                                    <HBox alignment="CENTER_LEFT" prefHeight="90.0" prefWidth="280.0">
                                       <children>
                                          <Button alignment="TOP_RIGHT" mnemonicParsing="false" text="Sort" textAlignment="RIGHT">
