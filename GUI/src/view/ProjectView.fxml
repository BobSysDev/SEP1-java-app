<VBox prefHeight="372.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ProjectViewController">
   <children>
      <HBox prefHeight="321.0" prefWidth="600.0">
