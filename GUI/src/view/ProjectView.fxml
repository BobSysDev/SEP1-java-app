                                          <Text nodeOrientation="LEFT_TO_RIGHT" strokeType="OUTSIDE" strokeWidth="0.0" text="Search:">
                                             <HBox.margin>
                                                <Insets left="20.0" right="20.0" />
                                             </HBox.margin>
                                             <font>
                                                <Font size="18.0" />
                                             </font>
                                          </Text>
                                          <TextField />
                                       </children>
                                       <opaqueInsets>
                                          <Insets />
                                       </opaqueInsets>
                                       <padding>
                                          <Insets top="10.0" />
                                       </padding>
                                    </HBox>
                                 </children>
                              </HBox>
                              <TableView fx:id="ongoingProjectListTable" layoutX="11.0" layoutY="46.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="247.0" prefWidth="573.0">
                                 <columns>
                                    <TableColumn fx:id="ongoingTypeColumn" prefWidth="75.0" text="Type" />
                                    <TableColumn fx:id="ongoingNameColumn" prefWidth="357.0" text="Name" />
                                    <TableColumn fx:id="ongoingDateColumn" prefWidth="140.0" text="Start date" />
                                 </columns>
                              </TableView>
                           </children>
                        </AnchorPane>
                     </content>
                  </Tab>
              </tabs>
            </TabPane>
         </children>
      </HBox>
      <HBox alignment="CENTER_RIGHT" prefHeight="51.0" prefWidth="600.0">
         <children>
            <Label fx:id="errorLabel" prefHeight="17.0" prefWidth="505.0" text="ERR:" />
            <Button onAction="#newProjectButtonPressed" mnemonicParsing="false" prefHeight="25.0" prefWidth="104.0" text="New Project">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
